{
  "name": "docsify-dynamo",
  "version": "1.0.0",
  "description": "docsify plugin for creating dynamic documentation sites",
  "repository": "git@github.com:illBeRoy/docsify-dynamo.git",
  "author": "Roy Sommer",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "parcel build",
    "test": "jest",
    "lint": "eslint .",
    "posttest": "yarn lint",
    "prerelase": "rm -rf dist && yarn build && yarn release",
    "release": "np"
  },
  "main": "dist/cjs/index.js",
  "umd": "dist/umd/dynamo.min.js",
  "types": "dist/types/index.d.ts",
  "targets": {
    "main": {
      "source": "src/index.ts",
      "context": "node",
      "isLibrary": true,
      "optimize": false
    },
    "umd": {
      "source": "src/umd.ts",
      "context": "browser",
      "optimize": true
    },
    "types": {
      "source": "src/index.ts",
      "context": "node",
      "isLibrary": true
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@parcel/packager-ts": "^2.2.1",
    "@parcel/transformer-typescript-types": "^2.2.1",
    "@types/ejs": "^3.1.0",
    "@types/memoizee": "^0.4.7",
    "eslint-config-typescript-prettier": "illberoy/eslint-typescript-prettier",
    "nock": "^13.2.2",
    "np": "^7.6.0",
    "parcel": "^2.2.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "ejs": "^3.1.6",
    "memoizee": "^0.4.15"
  }
}